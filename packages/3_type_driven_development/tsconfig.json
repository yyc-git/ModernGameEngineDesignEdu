{
	"compilerOptions": {
		"target": "ES6",
		"module": "ES6",
		"lib": [
			"DOM",
			"ESNext",
		],
		"moduleResolution": "node",
		"sourceMap": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"types": [],
		"noEmit": true,
		"noUnusedLocals": false,
		"noUnusedParameters":false,
		"strictNullChecks": true,
		"noImplicitReturns": true
	},
	"include": [
		"./",
	]
}